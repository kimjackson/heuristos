<?xml version="1.0" encoding="UTF-8"?>
<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">

 <map:views>
  <map:view from-label="xmlexport" name="raw">
   <map:serialize type="xml"/>
  </map:view>
  <map:view from-label="transformed" name="transformed">
   <map:serialize type="xml"/>
  </map:view>
  <map:view from-label="xinclude" name="xinclude">
   <map:serialize type="xml"/>
  </map:view>
  <map:view from-label="wordml" name="wordml">
   <map:transform src="xsl/tei_only.xsl"/>
   <map:serialize type="xml"/>
  </map:view>

  <map:view from-label="tei" name="tei">
   <map:serialize type="xml"/>
  </map:view>
 </map:views>

 <map:pipelines>
  <map:pipeline>

   <map:match pattern="item/*/raw">
    <map:redirect-to uri="../{1}/?cocoon-view=raw"/>
   </map:match>

   <map:match pattern="item/*/transformed">
    <map:redirect-to uri="../{1}/?cocoon-view=transformed"/>
   </map:match>

   <map:match pattern="item/*/wordml">
    <map:redirect-to uri="../{1}/?cocoon-view=wordml"/>
   </map:match>

   <map:match pattern="item/*/*">
    <map:generate src="http://dos.heuristscholar.org/heurist/php/xmlexport.php?q=ids:{1}&amp;w=all&amp;stype=&amp;rev=yes&amp;related_details=160,169,177,178,191,291,223,221,230,323,324,325,326,327,328,329,330,526,529&amp;depth=1"
     label="xmlexport"/>
    <map:transform src="xsl/item_view.xsl" label="transformed">
     <map:parameter name="use-request-parameters" value="true"/>
     <map:parameter name="id" value="{1}"/>
     <map:parameter name="related_reftype_filter" value="{2}"/>
    </map:transform>
    <map:transform type="xinclude" label="xinclude"/>
    <map:transform src="xsl/wordml2TEI.xsl" label="wordml"/>
    <map:transform src="xsl/tei_to_html_basic.xsl" label="tei"/>
    <map:serialize type="html"/>
   </map:match>

   <map:match pattern="item/*">
    <map:redirect-to uri="{1}/"/>
   </map:match>

   <map:match pattern="captioned">
    <map:generate src="http://dos.heuristscholar.org/heurist/php/xmlexport.php?pub_id=9"/>
	<map:transform src="xsl/annotations-basic.xsl"/>
    <map:serialize type="html"/>
   </map:match>

   <map:match pattern="uncaptioned">
    <map:generate src="http://dos.heuristscholar.org/heurist/php/xmlexport.php?pub_id=8"/>
	<map:transform src="xsl/annotations-basic.xsl"/>
    <map:serialize type="html"/>
   </map:match>

   <map:match pattern="unedited">
    <map:generate src="http://dos.heuristscholar.org/heurist/php/xmlexport.php?pub_id=12"/>
	<map:transform src="xsl/annotations-basic.xsl"/>
    <map:serialize type="html"/>
   </map:match>

   <map:match pattern="breadcrumbs/xml">
    <map:generate type="serverpages" src="xsp/breadcrumbs.xsp"/>
    <map:serialize type="xml"/>
   </map:match>

   <map:match pattern="breadcrumbs">
    <map:generate type="serverpages" src="xsp/breadcrumbs.xsp"/>
    <map:transform src="xsl/breadcrumbs.xsl">
     <map:parameter name="use-request-parameters" value="true"/>
	</map:transform>
    <map:serialize type="html"/>
   </map:match>

  </map:pipeline>
 </map:pipelines>
</map:sitemap>
